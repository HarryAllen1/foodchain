{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/2.1.0/schema.json",
  "global": {
    "fabricVersion": "2.5.9",
    "tls": false,
    "engine": "docker",
    "peerDevMode": false
  },
  "orgs": [
    {
      "organization": {
        "name": "main",
        "domain": "main.foodchain.com",
        "mspName": "mainMSP"
      },
      "orderers": [
        {
          "groupName": "maingroup",
          "type": "solo",
          "instances": 1
        }
      ],
      "peer": {
        "instances": 3
      },
      "tools": {
        "fabloRest": true
      }
    }
  ],
  "channels": [
    {
      "name": "supplychain",
      "orgs": [
        {
          "name": "main",
          "peers": [
            "peer0",
            "peer1",
            "peer2"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "main",
      "version": "0.0.1",
      "lang": "node",
      "channel": "supplychain",
      "directory": "./chaincodes/chaincode-kv-node"
    }
  ]
}