{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/2.1.0/schema.json",
  "global": {
    "fabricVersion": "2.5.9",
    "tls": false,
    "engine": "docker",
    "peerDevMode": false
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.foodchain.com",
        "mspName": "OrdererMSP"
      },
      "orderers": [
        {
          "groupName": "group1",
          "type": "solo",
          "instances": 1
        }
      ],
      "tools": {
        "fabloRest": true
      }
    },
    {
      "organization": {
        "name": "Farmers",
        "domain": "farmer.foodchain.com",
        "mspName": "FarmersMSP"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "tools": {
        "fabloRest": true
      }
    },
    {
      "organization": {
        "name": "Distributors",
        "domain": "distributor.foodchain.com",
        "mspName": "DistributorsMSP"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      },
      "tools": {
        "fabloRest": true
      }
    }
  ],
  "channels": [
    {
      "name": "my-channel1",
      "orgs": [
        {
          "name": "Farmers",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Distributors",
          "peers": [
            "peer0"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "chaincode1",
      "version": "0.0.1",
      "lang": "node",
      "channel": "my-channel1",
      "directory": "./chaincodes/chaincode-kv-node"
    }
  ]
}